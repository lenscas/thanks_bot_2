{
  "name": "thanks_bot_2",
  "version": "1.0.0",
  "description": "the second version of thanks_bot. Now in Typescript",
  "main": "build/src/index.js",
  "author": "Lenscas",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./build && ./node_modules/.bin/tsc",
    "typeCheck": "rimraf ./build && ./node_modules/.bin/tsc --noEmit",
    "start": "npm run build && node build/src/index.js",
    "prepareSql": "npx pgtyped -c postgres.json",
    "migrate": "./node_modules/.bin/db-migrate",
    "lintFix": "yarn lint --fix",
    "lint": "./node_modules/.bin/eslint 'src/**/*.{js,ts}'",
    "unitTest": "mocha -r ts-node/register test/**/*.spec.ts",
    "test": "yarn lint && yarn unitTest",
    "typedoc": "typedoc --mode modules --includeDeclarations",
    "typedocOpen": "yarn typedoc && firefox ./docs/index.html"
  },
  "dependencies": {
    "@pgtyped/query": "^0.10.2",
    "@types/node": "^14.14.13",
    "axios": "^0.21.0",
    "cli-table3": "^0.6.0",
    "discord.js": "^12.3.1",
    "pg": "^8.3.3",
    "typescript-pattern-matching": "^1.0.1"
  },
  "devDependencies": {
    "@pgtyped/cli": "^0.10.2",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/pg": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.2.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.1.0",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9"
  }
}