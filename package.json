{
  "name": "thanks_bot_2",
  "version": "1.0.0",
  "description": "the second version of thanks_bot. Now in Typescript",
  "main": "index.js",
  "author": "Lenscas",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./build && ./node_modules/.bin/tsc",
    "typeCheck": "rimraf ./build && ./node_modules/.bin/tsc --noEmit",
    "start": "npm run build && node build/index.js",
    "prepareSql": "npx pgtyped -c postgres.json",
    "migrate": "./node_modules/.bin/db-migrate",
    "lintFix": "./node_modules/.bin/eslint '*/**/*.{js,ts}' --fix",
    "lint": "./node_modules/.bin/eslint '**/*.{js,ts}' --fix",
    "unitTest": "mocha -r ts-node/register test/**/*.spec.ts",
    "test": "yarn lint && yarn unit-test"
  },
  "dependencies": {
    "@pgtyped/query": "^0.8.2",
    "@types/node": "^14.11.2",
    "discord.js": "^12.3.1",
    "pg": "^8.3.3"
  },
  "devDependencies": {
    "@pgtyped/cli": "^0.8.2",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/pg": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.3",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.1"
  }
}