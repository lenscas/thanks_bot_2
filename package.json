{
  "name": "thanks_bot_2",
  "version": "1.0.0",
  "description": "the second version of thanks_bot. Now in Typescript",
  "main": "build/src/index.js",
  "author": "Lenscas",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./build && ./node_modules/.bin/tsc",
    "typeCheck": "rimraf ./build && ./node_modules/.bin/tsc --noEmit",
    "start": "npm run build && node build/src/index.js",
    "prepareSql": "npx pgtyped -c postgres.json",
    "migrate": "./node_modules/.bin/db-migrate",
    "lintFix": "yarn lint --fix",
    "lint": "./node_modules/.bin/eslint 'src/**/*.{js,ts}'",
    "unitTest": "mocha -r ts-node/register test/**/*.spec.ts",
    "test": "yarn lint && yarn unitTest"
  },
  "dependencies": {
    "@pgtyped/query": "^0.10.2",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/node": "^16.10.3",
    "axios": "^0.21.0",
    "cli-table3": "^0.6.0",
    "discord.js": "^12.3.1",
    "fast-levenshtein": "^3.0.0",
    "get-urls": "^10.0.0",
    "pg": "^8.3.3",
    "typescript-pattern-matching": "^1.0.1"
  },
  "devDependencies": {
    "@pgtyped/cli": "^0.10.2",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/pg": "^8.6",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sinon": "^10",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^10",
    "typescript": "^3.9"
  }
}
